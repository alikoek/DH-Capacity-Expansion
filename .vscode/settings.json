{
    // Path to Julia executable
    "julia.executablePath": "/Users/jonathanhachez/.julia/juliaup/julia-1.10.10+0.aarch64.apple.darwin14/bin/julia",

    // Disable telemetry
    "julia.enableTelemetry": false,

    // Always start REPL automatically when running code
    "julia.startREPL": true,

    // Enable code cells (## or # %% delimiters)
